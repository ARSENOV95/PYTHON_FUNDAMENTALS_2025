def check_happiness(happyness_lst,factor):

    improved_happiness = [cur_val * factor for cur_val in happyness_lst]
    avg_happiness  = sum(improved_happiness) / (improved_happiness).count

    happy = list(filter(lambda x: x > avg_happiness,improved_happiness))

    if happy.count <improved_happiness.count:
        return f"{happy.count}/{improved_happiness.count}.\n Employees are not happy!"
    
    return f"{happy.count}/{improved_happiness.count()}.\n Employees are happy!"

happiness_lst = list(map(int,input().split()))
happiness_factor = int(input())

print(happiness_lst)
result = check_happiness(happiness_lst,happiness_factor)